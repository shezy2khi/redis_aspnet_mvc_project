@model IEnumerable<CacheRedisASP.Models.Transaction>


@{
    ViewData["Title"] = "Index";
}

<h2>First Time All Records from Database</h2>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AccountNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BanificiaryName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BankName)
            </th>

  @*          <th>
                @Html.DisplayNameFor(model => model.Amount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>*@
    
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AccountNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BanificiaryName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BankName)
                </td>

      @*          <td>
                    @Html.DisplayFor(modelItem => item.Amount)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>*@
                <td>
                    <a asp-action="Details" asp-route-id="@item.TransactionId" class="btn btn-info"><i class="alert-info"></i>  Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>
